{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@images/*": ["./src/assets/images/*"],

    },
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    // Astro directly run TypeScript code, no transpilation needed.
    "noEmit": true,
  }
}